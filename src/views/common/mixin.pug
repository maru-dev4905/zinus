mixin linkList(link, text)
    li
        a(href=link)=text

mixin list(texts)
    each text in texts
        li= text

mixin img(url, alt)
    img(src=url, alt=alt)

mixin wonderBox(url,stronger, texts)
    .wonder__box
        img(src=url, alt="wonder-img")
        .wonder__box__text
            strong= stronger
            p= texts